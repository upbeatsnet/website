                "CalvesAndHeifersExcludingDeathOrSoldCalc": {
                    "description": "The minimum number of calves+heifers when sold and dead animal percentages are not known",
                    "expression": {
                        "op": "$floor",
                        "target": {
                            "op": "$*",
                            "terms": [
                                "{/herd/herdNum}",
                                0.5
                            ]
                        }
                    }
                },
                "CalvesAndHeifersIncludingDeathOrSoldCalc": {
                    "description": "The minimum number of calves+heifers when sold and dead animal percentages are known",
                    "expression": {
                        "op": "$floor",
                        "target": {
                            "op": "$*",
                            "terms": [
                                "{/herd/herdNum}",

                "asFedOrDryMatter": {
                    "title": "As-fed or dry matter",
                    "type": "string",
                    "enum": [
                        "As-Fed",
                        "Dry Matter"
                    ]
                },
                "DryMatterCalc": {
                    "description": "Calculates the amount of dry matter in a feed within the context of an array of feeds",
                    "expression": {
                        "op": "$cond",
                        "if": {
                            "op": "$==",
                            "left": "{0/asFedDM}",
                            "right": "As-Fed"
                        },
                        "then": {
                            "op": "$round",
                            "precision": 1,
                            "target": {
                                "op": "$/",
                                "terms": [
                                    {
                                        "op": "$*",
                                        "terms": [
                                            "{0/amount}",
                                            {
                                                "op": "$findByIndex",
                                                "map": {
                                                    "$ref": "#/data/definitions/DryMatterMap/map"
                                                },
                                                "index": "{0/feed/value}"
                                            }
                                        ]
                                    },
                                    100
                                ]
                            }
                        },
                        "else": "{0/amount}"
                    }
                },
                "DryMatterCalcPercentLactatingCows": {
                    "description": "Calculates the percentage of dry matter in a feed within the context of an array of feeds for lactating cows",
                    "expression": {
                        "op": "$round",
                        "precision": 1,
                        "target": {
                            "op": "$*",
                            "terms": [
                                {
                                    "op": "$/",
                                    "terms": [
                                        {
                                            "$ref": "#/data/definitions/DryMatterCalc/expression"
                                        },
                                        "{/feeds/lactatingCowFeedsDMTally}"
                                    ]
                                },
                                100
                            ]
                        }
                    }
                },
                "DryMatterCalcPercentCalves": {
                    "description": "Calculates the percentage of dry matter in a feed within the context of an array of feeds for calves",
                    "expression": {
                        "op": "$round",
                        "precision": 1,
